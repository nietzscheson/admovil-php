{
    "name": "nietzscheson/admovil-php",
    "type": "library",
    "description": "Not-official Admovil API Client for PHP aplications.",
    "keywords": ["admovil", "sat", "cfdi", "api"],
    "license": "MIT",
    "authors": [
        {
            "name": "Cristian Angulo Nova <@nietzscheson>"
        }
    ],
    "require": {
        "php": "^7.1",
        "doctrine/common": "^2.8",
        "zendframework/zend-soap": "^2.6",
        "phplucidframe/console-table": "^1.2"
    },
    "require-dev": {
        "phpspec/phpspec": "4.3.0",
        "behat/behat": "dev-master",
        "fzaninotto/faker": "1.8",
        "symfony/options-resolver": "4.1.4"
    },
    "autoload": {
        "psr-4": { "Nietzscheson\\Admovil\\": "src" }
    },
    "autoload-dev": {
        "psr-4": { "Nietzscheson\\Admovil\\spec\\": "spec/" }
    }
}
